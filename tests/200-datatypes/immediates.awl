	// Test DB1.DATEx initial values
	L		DB1.DBW 16
	__ASSERT==	__ACCU 1,	4017
	L		DB1.DBW 18
	__ASSERT==	__ACCU 1,	L#40541


	CALL SFC 46 // STOP CPU
END_ORGANIZATION_BLOCK


